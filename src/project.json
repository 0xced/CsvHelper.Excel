{
  "name": "CsvHelper.Excel",
  "title": "CsvHelper for Excel",
  "version": "1.0.5",
  "authors": [
    "Chris Rodgers"
  ],
  "description": "An implementation of ICsvParser and ICsvSerializer from CsvHelper that reads and writes using the ClosedXml library.",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false
  },
  "dependencies": {
    "CsvHelper": "2.16.3",
    "ClosedXML": "0.80.1"
  },
  "frameworks": {
    "net40": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0"
      },
      "dependencies": {
        "DocumentFormat.OpenXml": "2.5.0"
      }
    },
    "net35": {
      "frameworkAssemblies": {
        "System.Data": "2.0.0.0"
      },
      "dependencies": {
        "DocumentFormat.OpenXml": "1.0.0"
      }
    }
  },
  "packOptions": {
    "summary": "Supports writing to and reading from Excel documents using CsvHelper and ClosedXml.",
    "owners": [
      "Chris Rodgers"
    ],
    "licenseUrl": "https://github.com/christophano/CsvHelper.Excel/blob/master/LICENSE",
    "projectUrl": "https://github.com/christophano/CsvHelper.Excel",
    "tags": [
      "CSV", "CSVHelper", "Excel", "ClosedXML"
    ]
  }
}
